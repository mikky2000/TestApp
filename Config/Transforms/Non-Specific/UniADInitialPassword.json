{
        "name": "Uni AD Initial Password",
        "type": "trim",
        "attributes": {
            "input": {
                "attributes": {
                    "newpass": {
                        "attributes": {
                            "length": "16"
                        },
                        "type": "randomAlphaNumeric"
                    },
                    "randomNumeric": {
                        "attributes": {
                            "length": "1"
                        },
                        "type": "randomNumeric"
                    },
                    "randomAlpha1": {
                        "attributes": {
                            "length": "1"
                        },
                        "type": "randomAlphaNumeric"
                    },
                    "randomAlpha2": {
                        "attributes": {
                            "length": "1"
                        },
                        "type": "randomAlphaNumeric"
                    },
                    "currentpass": {
                        "attributes": {
                            "values": [
                                {
                                    "attributes": {
                                        "name": "uniAdInitialPassword"
                                    },
                                    "type": "identityAttribute"
                                },
                                "Not Set"
                            ]
                        },
                        "type": "firstValid"
                    },
                    "value": "#if($currentpass.length() > 15)$currentpass#{else}${newpass}${randomNumeric}${randomAlpha1}.toUpperCase()${randomAlpha2}.toLowerCase()#end"
                },
                "type": "static"
            }
        },
        "internal": false
    }